    <Grid Background="">
   <Button x:Name="button" Content="Find a new home" HorizontalAlignment="Left" Margin="263,132,0,0" VerticalAlignment="Top" Width="119" Click="button_Click" RenderTransformOrigin="0.501,-3.271"/>
