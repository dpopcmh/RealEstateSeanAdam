        <Image HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100"/>
        <Button Content="Administration" HorizontalAlignment="Left" Margin="377,281,0,0" VerticalAlignment="Top" Width="119" Click="Button_Click_1"/>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="257,281,0,0" TextWrapping="Wrap" Text="Login&#x9;" VerticalAlignment="Top" Width="120"/>

